{
    "name": "makinacorpus/normalizer",
    "type": "library",
    "description": "PHP simple, fast and flexible object normalizer",
    "keywords": ["normalizer", "serialization", "symfony"],
    "homepage": "http://github.com/pounard/php-normalizer",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name" : "Makina Corpus",
            "homepage" : "http://makina-corpus.com"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "autoload": {
        "classmap": [
            "src/Cache.php",
            "src/Context.php",
            "src/CustomNormalizer.php",
            "src/DefaultNormalizer.php",
            "src/Error.php",
            "src/Normalizer.php",
            "src/Options.php",
            "src/Strategy.php",
            "src/TypeDefinition.php"
        ],
        "files": [
            "src/Generator/Iteration1-Reference.php",
            "src/Generator/Iteration2-Generation.php",
            "src/Generator/Iteration3-Helper.php",
            "src/Generator/Iteration3-Reference.php",
            "src/Generator/Iteration4-Generation.php"
        ],
        "psr-4": {
            "MakinaCorpus\\Normalizer\\": "src/",
            "MakinaCorpus\\Normalizer\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "benchmarks/CommonTrait.php",
            "benchmarks/Definitions.php"
        ],
        "psr-4": {
            "Generated2\\MakinaCorpus\\Normalizer\\Benchmarks\\": "benchmarks/Generated2/MakinaCorpus/Normalizer/Benchmarks/",
            "Generated2\\MakinaCorpus\\Normalizer\\Tests\\Functional\\": "tests/Generated2/MakinaCorpus/Normalizer/Tests/Functional/",
            "Generated4\\MakinaCorpus\\Normalizer\\Benchmarks\\": "benchmarks/Generated4/MakinaCorpus/Normalizer/Benchmarks/",
            "Generated4\\MakinaCorpus\\Normalizer\\Tests\\Functional\\": "tests/Generated4/MakinaCorpus/Normalizer/Tests/Functional/",
            "MakinaCorpus\\Normalizer\\Benchmarks\\": "benchmarks/",
            "MakinaCorpus\\Normalizer\\Tests\\": "tests/"
        }
    },
    "require": {
        "makinacorpus/generated-hydrator": "dev-master",
        "makinacorpus/goat-hydrator": "^1.0.0-alpha6",
        "symfony/property-info": "^4.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpbench/phpbench": "^1.0",
        "phpunit/phpunit": "^7",
        "ramsey/uuid": "^4.0",
        "symfony/cache": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/property-access": "^4",
        "symfony/serializer": "^4.3",
        "symfony/yaml": "^4",
        "symfony/config": "^4.3",
        "symfony/http-kernel": "^4.3"
    },
    "suggest": {
        "ramsey/uuid": "For UUID database column type support",
        "symfony/property-info": "For docblock based object type introspection",
        "symfony/serializer": "This normalizer can be used as a replacement for symfony normalizer"
    }
}
